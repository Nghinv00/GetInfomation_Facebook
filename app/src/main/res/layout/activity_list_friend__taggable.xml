<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_list_friend__taggable"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.nghinv.getinfomation_facebook.ListFriend_Taggable">


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Danh Sách bạn bè có cùng taggable "
        android:gravity="left"
        android:textSize="15dp"
        android:id="@+id/txtTaggable"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/txtTag"
        android:text=""
        android:layout_below="@id/txtTaggable"/>

    <ListView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/lvlistTag"
        android:layout_below="@id/txtTag"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

    </ListView>
</RelativeLayout>
